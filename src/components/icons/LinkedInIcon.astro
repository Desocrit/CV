---
/**
 * LinkedIn icon (stroke-based)
 * Matches the stroke style of other sidebar icons
 */
---
<svg fill="none" stroke="currentColor" viewBox="0 0 24 24" aria-hidden="true">
  <!-- Outer rounded square -->
  <rect x="3" y="3" width="18" height="18" rx="2" stroke-width="1.5" />
  <!-- Profile dot -->
  <circle cx="7.5" cy="7.5" r="1.5" fill="currentColor" stroke="none" />
  <!-- Profile line -->
  <path stroke-linecap="round" stroke-width="1.5" d="M7.5 11v6" />
  <!-- Connection lines -->
  <path stroke-linecap="round" stroke-linejoin="round" stroke-width="1.5" d="M11.5 17v-4c0-1.5 1-2.5 2.5-2.5s2.5 1 2.5 2.5v4" />
  <path stroke-linecap="round" stroke-width="1.5" d="M11.5 11v-0.5" />
</svg>
